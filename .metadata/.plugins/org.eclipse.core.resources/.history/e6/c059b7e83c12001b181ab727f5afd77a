package eg1;

public class Demo1 {
	public static void main(String[] args) {
		System.out.println("Hello");
		myStaticMethod();
		
		// Anything which is non-static create object of that class to access it;
		// Object creation Syntax ClassName objName=new Constructor();
		Demo1 d = new Demo1();
		d.myNonStaticMethod();
	}
	
	public static void myStaticMethod() {
		System.out.println("Hello from myStaticMethod()");
	}
	
	public void myNonStaticMethod() {
		System.out.println("Hello from myNonStaticMethod()");
	}
}
